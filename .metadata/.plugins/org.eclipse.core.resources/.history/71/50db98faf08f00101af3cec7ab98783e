
class OuterrClass {
int x = 10;

          class MiddleeClass {
                  int y = 5;
                  
                class InnerClass {
                           int z = 1;
             }
          }
}

class Nested_Innerclass {
       public static void main(String[] args) {
              OuterClass Out1 = new OuterClass();
              OuterrClass.MiddleeClass Mid1 = new OuterClass().new MiddleeClass();
             OuterrClass.MiddleeClass.InnerClass In1 = new OuterClass().new MiddleeClass().new InnerClass();
                 System.out.println(In1.z + Mid1.y + Out1.x); 
     }

}

//In Java, it is also possible to nest classes (a class within a class).